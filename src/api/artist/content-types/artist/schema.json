{
  "kind": "collectionType",
  "collectionName": "artists",
  "info": {
    "singularName": "artist",
    "pluralName": "artists",
    "displayName": "Artist"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "required": true
    },
    "description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "required": true
    },
    "dob": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "date",
      "required": true
    },
    "director": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "required": false,
      "default": "director"
    },
    "productionHouse": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string"
    },
    "picture": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "instagram": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string"
    },
    "facebook": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string"
    },
    "twitter": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string"
    },
    "characters": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::character.character",
      "mappedBy": "artists"
    },
    "movies": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::movie.movie",
      "mappedBy": "creators"
    },
    "comics": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::comic.comic",
      "mappedBy": "creators"
    }
  }
}
